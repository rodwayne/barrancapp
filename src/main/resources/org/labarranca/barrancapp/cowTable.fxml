<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/22"
      fx:controller="org.labarranca.barrancapp.controllers.CowTableController"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="addCowButton" mnemonicParsing="false" text="Agregar Vaca" onAction="#openAddCowForm"/>
        <TableView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
            <columns>
                <TableColumn prefWidth="140.0" text="Arete"/>
                <TableColumn minWidth="0.0" prefWidth="140.0" text="Raza"/>
                <TableColumn minWidth="0.0" prefWidth="140.0" text="Sexo"/>
                <TableColumn prefWidth="140.0" text="Peso"/>
            </columns>
        </TableView>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
